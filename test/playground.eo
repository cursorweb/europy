fn while_loop(n) {
    var start = clock();

    var i = 0, s = 0;
    while i < n {
        s += i;
        i += 1;
    }

    return clock() - start;
}

fn for_loop(n) {
    var start = clock();

    var i = 0, s = 0;
    for i in 0..n {
        s += i;
        i += 1;
    }

    return clock() - start;
}

println("while " + while_loop(10000) + "ms");
println("for " + while_loop(10000) + "ms");