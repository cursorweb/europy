use clock;
use io.println;

fn while_loop(n) {
    var now = clock.now();

    var i = 0, s = 0;
    while i < n {
        s += i;
        i += 1;
    }

    return now - clock.now();
}

fn for_loop(n) {
    var now = clock.now();

    var s = 0;
    for i in 0..n {
        s += i;
    }

    return now - clock.now();
}

println("while " + while_loop(2));
println("for " + while_loop(2));